public class averageage
{
    public static void main(String[] args)
    {
        float[] age={22,18,35,48,26,87,70,20};
        float sum=0;
        float a=age.length;
        for (int i=0;i<a;i++)
        {
            sum=sum+age[i];
        }
        float result=sum/a;
        System.out.println(result);
    }
}
